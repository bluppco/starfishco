---
// IMPORTS BENCHMARKING FROM MD
import { frontmatter as benchmarking } from "@/content/benchmarking/home.mdx"

// IMPORT LAYOUTS
import Layout from "@/layouts/Layout.astro"
import Container from "@/layouts/Container.astro"

// IMPORT COMPONENTS
import Footer from "@/components/common/Footer.astro"
import Header from "@/components/common/Nav.astro"
import BenchmarkingJSX from "@/components/benchmarking"
import HeaderBanner from "@/components/common/HeaderBanner.astro"
---
<Layout
	title={ "Benchmarking" }
	description={ "Benchmarking" }
	image=""
>
    <HeaderBanner />    
    <Header />

    <section class="py-40">
        <Container>
            <BenchmarkingJSX client:load data={ benchmarking }/>
        </Container>
    </section>

    <Footer />
</Layout>
